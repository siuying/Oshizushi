OSZExpression    ::= direction@<orientation>? leading@(<edge> <OSZPadding>?)? <OSZView> trailing@(<OSZPadding>? <edge>)?;
OSZViewSet       ::=  <OSZPadding>? <OSZView>;
OSZView          ::= '[' name@'Identifier' ']';
OSZPadding       ::= '-' 'Number' '-' | '-';
3 orientation    ::= 'V:';
2 orientation    ::= 'H:';
1 edge           ::= '|';