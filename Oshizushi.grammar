OSZExpression    ::= direction@<orientation>? leading@(<edge> <OSZConnection>?)? <OSZView> trailing@(<OSZConnection>? <edge>)?;
OSZViewSet       ::=  <OSZConnection>? <OSZView>;
OSZView          ::= '[' name@'Identifier' ']';
OSZConnection    ::= '-' 'Number' '-';
OSZConnection    ::= '-';
3 orientation    ::= 'V:';
2 orientation    ::= 'H:';
1 edge           ::= '|';